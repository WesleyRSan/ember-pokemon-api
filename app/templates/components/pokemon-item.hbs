<div class="col-md-4 col-lg-3 pokemon-item">
  <div class="thumbnail home-thumbnail alert alert-info">
    {{#if pokemon.image}}
      <img class="home-thumbnail__img2" alt="foto {{pokemon.name}}" src="{{pokemon.image}}" data-holder-rendered="true">
    {{else}}
      <img class="home-thumbnail__img" alt="foto {{pokemon.name}}" src="/images/loading-image.gif" data-holder-rendered="true">
    {{/if}}
    <div class="caption">
      <h3>{{pokemon.name}}</h3>
    </div>
    <a href="{{urlPokemonDetail}}" class="btn btn-primary" role="button">detalhes</a>
  </div>
</div>